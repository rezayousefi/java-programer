public class BreakTest
{
    public static void main (String[] args)
    {
        int count;

        for (count = 1; count <= 10; count++ )
        {
            if (count == 5)
                break;

            System.out.printf("%D " , count);

        }
        System.out.printf("\nBorke out of loop at count = %d\n" , count);

    }

}
