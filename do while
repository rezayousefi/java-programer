import java.io.PrintStream;

/**
 * Created by Reza on 17/07/2014.
 */
public class DoWhileTest
{
    public static void main (String [] args)
    {
        int counter = 1;
        do {
            System.out.printf("%d " , counter);
            ++counter;
        } while (counter <= 10);

        System.out.println();
    }
}
